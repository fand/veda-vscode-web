{
    "manifest_version": 2,
    "name": "VEDA for code-server",
    "version": "0.0.0",
    "description": "GLSL livecoder",
    "icons": {
      "16": "images/16.png",
      "128": "images/128.png"
    },
    "author": "Takayosi Amagi <fand.gmork@gmail.com>",
    "background": {
      "persistent": false,
      "scripts": ["index.js"]
    },
    "permissions": ["activeTab", "storage", "nativeMessaging"],
    "browser_action": {
      "default_icon": {
        "19": "images/19.png",
        "38": "images/38.png"
      },
      "default_title": "VEDA for code-server"
    }
  }
